{
  "name": "super github",
  "description": "a chrome extension to improve github user experience",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/dotnet/docfx/compare/*"
      ],
      "js": [
        "vendor/jquery.slim.min.js",
        "background.js"
      ]
    }
  ],
  "browser_action": {
    "default_title": "super github",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "manifest_version": 2
}